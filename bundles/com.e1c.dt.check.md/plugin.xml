<plugin>
    <extension point="com.e1c.g5.v8.dt.check.checks">
    
      <category
          id="com.e1c.dt.check.md"
          description="%category.md.description"
          title="%category.md.title"
          category="com.e1c.g5.v8.dt.check.edt.checks">
      </category>
 
      <check
          category="com.e1c.dt.check.md"
          class="com.e1c.dt.check.internal.md.ExecutableExtensionFactory:com.e1c.dt.check.internal.md.check.ConfigurationCompatibilityModeCheck">
      </check>
      
      <check
          category="com.e1c.dt.check.md"
          class="com.e1c.dt.check.internal.md.ExecutableExtensionFactory:com.e1c.dt.check.internal.md.check.ConfigurationUsedMobileApplicationFunctionalitiesCheck">
      </check>
      
      <check
          category="com.e1c.dt.check.md"
          class="com.e1c.dt.check.internal.md.ExecutableExtensionFactory:com.e1c.dt.check.internal.md.check.ConfigurationMobileApplicationUrlsCheck">
      </check>
      
      <check
          category="com.e1c.dt.check.md"
          class="com.e1c.dt.check.internal.md.check.MdReferenceIntegrity">
      </check>
      
      <check
          category="com.e1c.dt.check.md"
          class="com.e1c.dt.check.internal.md.ExecutableExtensionFactory:com.e1c.dt.check.internal.md.check.ConfigurationStandaloneContentCheck">
      </check>
                  
    </extension>
    
    <extension point="com.e1c.g5.v8.dt.check.fixes">
        <fix
            class="com.e1c.dt.check.internal.md.fix.ConfigurationStandaloneContentFix">
        </fix>
   </extension>
</plugin>